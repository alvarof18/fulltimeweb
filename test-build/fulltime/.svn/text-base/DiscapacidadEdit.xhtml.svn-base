<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
    xmlns:s="http://jboss.com/products/seam/taglib"
    xmlns:ui="http://java.sun.com/jsf/facelets"
    xmlns:f="http://java.sun.com/jsf/core"
    xmlns:h="http://java.sun.com/jsf/html"
    xmlns:a="http://richfaces.org/a4j"
    xmlns:rich="http://richfaces.org/rich"
    template="#{requestScope.contextPath}/layout/template.xhtml">

<ui:define name="body">

    <h:form id="discapacidad" styleClass="edit">

        <rich:panel>
            <f:facet name="header">#{discapacidadHome.managed ? 'Editar' : 'A&#241;adir'} Discapacidad</f:facet>




            <s:decorate id="carnetConadisField" template="#{requestScope.contextPath}/layout/edit.xhtml">
                <ui:define name="label">Carnet conadis</ui:define>
                <h:inputText id="carnetConadis" size="100"
                              value="#{discapacidadHome.instance.carnetConadis}"/>
            </s:decorate>


            <s:decorate id="porcentajeField" template="#{requestScope.contextPath}/layout/edit.xhtml">
                <ui:define name="label">Porcentaje</ui:define>
                <h:inputText id="porcentaje"
                          value="#{discapacidadHome.instance.porcentaje}"
                           size="13">
                    <a:support event="onblur" reRender="porcentajeField" bypassUpdates="true" ajaxSingle="true"/>
                </h:inputText>
            </s:decorate>


            <s:decorate id="tipoField" template="#{requestScope.contextPath}/layout/edit.xhtml">
                <ui:define name="label">Tipo</ui:define>
                <h:inputText id="tipo"
                          value="#{discapacidadHome.instance.tipo}">
                    <a:support event="onblur" reRender="tipoField" bypassUpdates="true" ajaxSingle="true"/>
                </h:inputText>
            </s:decorate>

            <div style="clear:both">
                <span class="required">*</span>
                Campos requeridos
            </div>

        </rich:panel>

        <div class="actionButtons">

            <h:commandButton id="save"
                          value="Grabar"
                         action="#{discapacidadHome.persist}"
                       disabled="#{!discapacidadHome.wired}"
                       rendered="#{!discapacidadHome.managed}"/>

            <h:commandButton id="update"
                          value="Grabar"
                         action="#{discapacidadHome.update}"
                       rendered="#{discapacidadHome.managed}"/>

            <h:commandButton id="delete"
                          value="Eliminar"
                         action="#{discapacidadHome.remove}"
                      immediate="true"
                       rendered="#{discapacidadHome.managed}"/>

            <s:button id="cancelEdit"
                   value="Cancelar"
             propagation="end"
                    view="/fulltime/Discapacidad.xhtml"
                rendered="#{discapacidadHome.managed}"/>

            <s:button id="cancelAdd"
                   value="Cancelar"
             propagation="end"
                    view="/fulltime/#{empty discapacidadFrom ? 'DiscapacidadList' : discapacidadFrom}.xhtml"
                rendered="#{!discapacidadHome.managed}"/>

        </div>
    </h:form>

