	<parameter name="empresa" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     grupos.descripcion AS grupos_descripcion,
     subgrupos.descripcion AS subgrupos_descripcion,
     item_menu.descripcion AS item_menu_descripcion,
     item_menu.link AS item_menu_link
FROM
     grupos grupos INNER JOIN subgrupos subgrupos ON grupos.id = subgrupos.grupos
     INNER JOIN item_menu item_menu ON grupos.id = item_menu.grupos
     AND subgrupos.id = item_menu.subgrupos
WHERE
     lower(item_menu.descripcion) LIKE $P{descripcion}
ORDER BY grupos.descripcion, subgrupos.descripcion, item_menu.descripcion

]]>
	</queryString>
